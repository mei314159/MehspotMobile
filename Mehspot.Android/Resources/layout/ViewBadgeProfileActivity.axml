<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <FrameLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:id="@+ViewBadgeProfileActivity/PictureFrame">
        <ImageView
            android:layout_width="50dp"
            android:id="@+ViewBadgeProfileActivity/Picture"
            android:src="@drawable/profile_image"
            android:scaleType="centerCrop"
            android:layout_height="50dp"
            android:background="@drawable/roundedframe"
            android:padding="0dp" />
    </FrameLayout>
    <ImageView
        android:src="@+drawable/staryellow"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:id="@+ViewBadgeProfileActivity/FavoriteIcon"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="40dp" />
    <TextView xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+ViewBadgeProfileActivity/UserNameLabel"
        android:layout_alignParentTop="true"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="70dp"
        android:layout_alignParentRight="true"
        tools:text="User Name"
        android:textStyle="bold"
        android:textSize="15dp"
        android:layout_toRightOf="@+ViewBadgeProfileActivity/PictureFrame" />
    <TextView xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+ViewBadgeProfileActivity/LikesCount"
        tools:text="0"
        android:layout_below="@+ViewBadgeProfileActivity/UserNameLabel"
        android:layout_toRightOf="@+ViewBadgeProfileActivity/PictureFrame"
        android:layout_marginLeft="10dp"
        android:textSize="11dp" />
    <TextView xmlns:p2="http://schemas.android.com/tools"
        android:id="@+ViewBadgeProfileActivity/LikesCountText"
        android:textSize="11dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+ViewBadgeProfileActivity/LikesCount"
        android:layout_below="@+ViewBadgeProfileActivity/UserNameLabel"
        android:text="Likes /"
        android:layout_marginLeft="3dp" />
    <TextView xmlns:p2="http://schemas.android.com/tools"
        android:id="@+ViewBadgeProfileActivity/RecommendationsCount"
        p2:text="0"
        android:textSize="11dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="3dp"
        android:layout_toRightOf="@+ViewBadgeProfileActivity/LikesCountText"
        android:layout_below="@+ViewBadgeProfileActivity/UserNameLabel" />
    <TextView xmlns:p2="http://schemas.android.com/tools"
        android:id="@+ViewBadgeProfileActivity/RecommendationsCountText"
        android:textSize="11dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+ViewBadgeProfileActivity/RecommendationsCount"
        android:layout_below="@+ViewBadgeProfileActivity/UserNameLabel"
        android:text="Recommendations"
        android:layout_marginLeft="3dp" />
    <TextView xmlns:p2="http://schemas.android.com/tools"
        android:id="@+ViewBadgeProfileActivity/SubdivisionLabel"
        android:textSize="13dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+ViewBadgeProfileActivity/PictureFrame"
        android:layout_below="@+ViewBadgeProfileActivity/LikesCount"
        android:layout_marginLeft="10dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="10dp"
        p2:text="Country Club of Roswell (30076)" />
    <TextView xmlns:p2="http://schemas.android.com/tools"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+ViewBadgeProfileActivity/InfoLabel1"
        p2:text="$0/hr"
        android:textSize="11dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="10dp"
        android:layout_marginRight="10dp"
        android:textColor="#7EB643" />
    <TextView xmlns:p2="http://schemas.android.com/tools"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+ViewBadgeProfileActivity/DistanceLabel"
        p2:text="4.36 miles"
        android:textSize="11dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="10dp"
        android:textColor="#EF8748"
        android:layout_below="@+ViewBadgeProfileActivity/InfoLabel1" />
    <TextView xmlns:p2="http://schemas.android.com/tools"
        android:id="@+ViewBadgeProfileActivity/InfoLabel2"
        android:textSize="12dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+ViewBadgeProfileActivity/PictureFrame"
        android:layout_below="@+ViewBadgeProfileActivity/SubdivisionLabel"
        android:layout_marginLeft="10dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="10dp"
        p2:text="High School"
        android:textColor="#ff9e9e9e"
        android:layout_marginBottom="10dp" />
    <LinearLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+ViewBadgeProfileActivity/InfoWrapper"
        android:layout_below="@+ViewBadgeProfileActivity/InfoLabel2"
        android:layout_marginBottom="10dp" />
</RelativeLayout>